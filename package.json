{
  "name": "vartiq",
  "version": "1.0.4",
  "description": "TS/JS SDK for Vartiq",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vartiq",
    "npm vartiq",
    "vartiq nodejs sdk",
    "vartiq typescript sdk",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "author": "Atikur Rahaman",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@vitest/ui": "^3.2.2",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vitest": "^3.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
